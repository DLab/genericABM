<?xml version="1.0" encoding="UTF-8"?>
<Entity xmlns="http://www.example.org/Entity" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.example.org/Entity Entity.xsd "
	tableName="grupo_view">

  <field property="codigo"                sqlName="grp_codigo"         		type="String"  key="true"/>
  <field property="codigoTipo"            sqlName="grp_tipo"   		        type="String" akey="true" />
  <field property="descripcion"      	  sqlName="grp_descripcion"    	    type="String"/>
  <field property="correlativo"           sqlName="gvr_correlativo"         type="Integer"  key="true"/>
  <field property="inicio"             	  sqlName="gvr_inicio"      		type="Timestamp"/>
  <field property="codigoEstado"          sqlName="gvr_est_codigo"   		type="String"/>
  <field property="expresion"      	  	  sqlName="gvr_expresion"    	    type="String"/>
  <field property="comentario"            sqlName="gvr_comentario"         	type="String"/>
  <field property="usuario"               sqlName="gvr_usuario"        		type="String"/>
  <field property="timestamp"             sqlName="gvr_timestamp"      		type="Timestamp"/>
  <field property="estado"                sqlName="est_descripcion"  		type="String"/>
  <field property="maxCorrelativo"        sqlName="max_correlativo"         type="Integer"  key="true"/>
 

  <query id="activos">
  	 <field property="codigoTipo"/>
  	 <field property="codigoEstado"/>
  </query> 
</Entity>
