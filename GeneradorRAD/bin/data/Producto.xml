<?xml version="1.0" encoding="UTF-8"?>
<Entity xmlns="http://www.example.org/Entity" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.example.org/Entity Entity.xsd "
	tableName="producto">

  <field property="codigo"                sqlName="prd_codigo"         	 type="String"  key="true"/>
  <field property="codigoModulo"          sqlName="prd_codmodulo"    	 type="String"/>
  <field property="codigoCliente"         sqlName="prd_codcliente"  	 type="String"/>
  <field property="codigoEstado"      	  sqlName="prd_est_codigo"  	 type="String"/>
  <field property="descripcion"           sqlName="prd_descripcion"      type="String"/>
  <field property="comentario"            sqlName="prd_comentario"    	 type="String"/>
  <field property="codigoMoneda"          sqlName="prd_prm_moneda"   	 type="String"/>
  <field property="codigoSistema"         sqlName="prd_prm_sistema"   	 type="String"/>
  <field property="codigoTipoSubtipo"     sqlName="prd_prm_tipo_subtipo" type="String"/>
  <field property="usuario"               sqlName="prd_usuario"        	 type="String"/>
  <field property="timestamp"             sqlName="prd_timestamp"        type="Timestamp"/>
  
  <query id="activos">
  	 <field property="codigoEstado"/>
  </query> 
  
</Entity>
